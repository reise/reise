        "@types/morgan": "^1.7.32",
        "@types/node": "^6.0.81",
        "aglio": "^2.3.0",
        "typescript": "^2.4.1"
    },
    "dependencies": {
        "@types/connect-mongo": "0.0.33",
        "@types/express-session": "^1.15.6",
