{
    "name": "reise",
    "version": "1.0.0",
    "description": "An application for online travel booking.",
    "main": "server.js",
    "scripts": {
        "start": "npm run config && npm run tsc && npm run ui && node dist/server.js",
        "ui":"cd ui && node_modules/@angular/cli/bin/ng build",
        "config": "npm i && cd ui && npm i",
        "tsc": "node_modules/typescript/bin/tsc",
        "mongo": "sudo service mongod stop && sudo mongod --dbpath data/",
        "server": "npm run tsc && node dist/server.js",
        "docs": "aglio --theme-variables default -i ./docs/api.apib --full-width -o ./docs/api.html"
    },
